



CollisionShape.SetEnabled(Scene.Find("portal"), 106, false);

if Character.IsQuestCompleted("quest02_village_elder") then
    Scene.SetGroupHidden("block_group", true);
    Scene.SetGroupHidden("pass_group", false);
end

if Character.IsQuestCompleted("quest01_find_master") then
    Scene.SetGroupHidden("day", false);
    Scene.SetGroupHidden("night", true);
    CollisionShape.SetEnabled(Scene.Find("portal"), 106, true);
    Program.Wait(0.01);
else
    Scene.SetGroupHidden("day", true);
    Scene.SetGroupHidden("night", false);
    Program.Wait(0.01);
end



